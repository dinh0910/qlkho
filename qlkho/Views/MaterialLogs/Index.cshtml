@model IEnumerable<qlkho.Models.MaterialLog>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Material)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stored)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TakeAway)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TookAway)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Returned)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Material.MaterialID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stored)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TakeAway)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TookAway)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Returned)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.UserID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaterialLogID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaterialLogID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaterialLogID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
